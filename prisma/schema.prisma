generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // 사용 중인 DB가 MySQL이라면 "mysql"로 변경
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
}
